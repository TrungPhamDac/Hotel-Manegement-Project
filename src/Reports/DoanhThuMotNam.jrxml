	<queryString>
		<![CDATA[select temp2.mathanhtoan, temp2.THANHTIEN, temp2.madatphong, temp2.tienphong, temp2.matiec,  temp2.tongtientiec, hddv.MAHDDV, hddv.THANHTIEN tongtiendv, temp2.thang
from
(select temp1.mathanhtoan, temp1.THANHTIEN, temp1.madatphong, temp1.tienphong, hdt.matiec, hdt.THANHTIEN tongtientiec, temp1.thang
from
(select tt.mathanhtoan, tt.THANHTIEN, pdp.madatphong, pdp.tienphong , extract(month from ngaylap) thang
from thanhtoan tt, phieudatphong pdp
where tt.madatphong = pdp.madatphong and extract(year from ngaylap)= $P{nam}) temp1
 LEFT OUTER JOIN hoadontiec hdt ON hdt.madatphong = temp1.madatphong ) temp2
 LEFT OUTER JOIN hoadondv hddv ON hddv.madatphong = temp2. madatphong]]>
	</queryString>
