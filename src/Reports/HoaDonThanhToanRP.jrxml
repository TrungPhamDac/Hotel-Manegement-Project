	<parameter name="madatphong" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select q2.madatphong, q2.tienphong, q2.MAPHG, q2.DONGIAPHONG, q2.mahddv, q2.mpdv, q2.ttdv, t.matiec, t.maphg mpt, t.thanhtien tttiec, get_tongtien_thanhtoan(q2.madatphong) tt, ngaydat, ngaynhan, ngaytra
from (select q1.madatphong, q1.tienphong, q1.MAPHG, q1.DONGIAPHONG, dv.mahddv, dv.maphg mpdv, dv.thanhtien ttdv, ngaydat, ngaynhan, ngaytra
from (select dp.madatphong, dp.tienphong, ct.MAPHG, ct.DONGIAPHONG, ngaydat, ngaynhan, ngaytra from phieudatphong dp join chitietdatphong ct on dp.madatphong = ct.madatphong) q1
left outer join (select madatphong, mahddv, maphg, thanhtien from hoadondv) dv on dv.madatphong = q1.madatphong) q2 left outer join (select madatphong, matiec, maphg, thanhtien from hoadontiec where tinhtrang =0) t on t.madatphong = q2.madatphong
where q2.madatphong =$P{madatphong}]]>
	</queryString>
